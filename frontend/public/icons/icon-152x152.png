<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 512 512">
  <defs>
    <linearGradient id="grad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#667eea;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#764ba2;stop-opacity:1" />
    </linearGradient>
  </defs>

  <!-- Background -->
  <rect width="512" height="512" rx="115" fill="url(#grad)"/>

  <!-- Road/Highway symbol -->
  <g transform="translate(256, 256)">
    <!-- Highway shield -->
    <path d="M -80 -120 L -100 -80 L -100 80 L -80 120 L 80 120 L 100 80 L 100 -80 L 80 -120 Z"
          fill="white" stroke="#2563eb" stroke-width="8"/>

    <!-- Road lines -->
    <rect x="-60" y="-100" width="30" height="200" fill="#2563eb" opacity="0.3"/>
    <rect x="30" y="-100" width="30" height="200" fill="#2563eb" opacity="0.3"/>

    <!-- Center dashed line -->
    <rect x="-10" y="-90" width="20" height="30" fill="#fbbf24"/>
    <rect x="-10" y="-40" width="20" height="30" fill="#fbbf24"/>
    <rect x="-10" y="10" width="20" height="30" fill="#fbbf24"/>
    <rect x="-10" y="60" width="20" height="30" fill="#fbbf24"/>

    <!-- Communication waves -->
    <circle cx="-70" cy="-60" r="15" fill="none" stroke="#10b981" stroke-width="4"/>
    <circle cx="-70" cy="-60" r="25" fill="none" stroke="#10b981" stroke-width="3" opacity="0.6"/>
    <circle cx="-70" cy="-60" r="35" fill="none" stroke="#10b981" stroke-width="2" opacity="0.3"/>

    <circle cx="70" cy="60" r="15" fill="none" stroke="#10b981" stroke-width="4"/>
    <circle cx="70" cy="60" r="25" fill="none" stroke="#10b981" stroke-width="3" opacity="0.6"/>
    <circle cx="70" cy="60" r="35" fill="none" stroke="#10b981" stroke-width="2" opacity="0.3"/>
  </g>

  <!-- DOT text -->
  <text x="256" y="420" font-family="Arial, sans-serif" font-size="60" font-weight="bold"
        text-anchor="middle" fill="white">DOT</text>
</svg>
